{
  "name": "repo-search",
  "version": "1.0.0",
  "description": "A search engine for both beginners and experts to find git repos to contribute to.",
  "main": "web/js/main.jsx",
  "scripts": {
    "build": "npx webpack --config webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx webpack serve --open --config webpack.dev.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d bundle",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasrpatten/repo-search.git"
  },
  "keywords": [
    "Git",
    "GitHub",
    "Search",
    "Git Search",
    "GitHub Search",
    "Search Engine",
    "React"
  ],
  "author": "lucasrpatten",
  "license": "MIT",
  "homepage": "https://lucasrpatten.github.io/repo-search/",
  "bugs": {
    "url": "https://github.com/lucasrpatten/repo-search/issues"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "jquery": "^3.2.1",
    "prettier": "2.6.2",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "style-loader": "^0.18.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "quoteProps": "consistent"
  },
  "lint-staged": {
    "**/*": "prettier --write ."
  }
}
